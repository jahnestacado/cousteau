{
  "name": "cousteau",
  "version": "1.0.6",
  "description": "Recursive directory walker that retrieves all sub-directory and file paths along with their stats.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "babel": "gulp",
    "test-on-travis": "istanbul cover --report lcovonly _mocha",
    "upload-to-coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "recursive",
    "search",
    "symlink",
    "symbolic",
    "link",
    "stats",
    "fs",
    "readdir",
    "filesystem",
    "find",
    "filter"
  ],
  "author": "Ioannis Tzanellis <ioannis.kiwe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-shallow-deep-equal": "^1.4.4",
    "coveralls": "^2.11.16",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mock-fs": "^3.12.1"
  }
}
